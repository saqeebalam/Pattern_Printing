package SolidPattern;

public class Delete {
	public static void main(String[] args) {
		 
		//invert pyramid code
		 /* ****
		     ***
		      **
		       * */
		int m=4;
		for(int i=1;i<=4;i++) {
			for(int j=1;j<=m;j++) {
				System.out.print("*");
			}
			m--;
			System.out.println(" ");
		}
		
		
	}

}
